{
  "problemMatcher": [
    {
      "severity": "error",
      "pattern": [
        {
          "regexp": "^([^:]+):(\\d+):(\\d+): (E\\d+): (.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "code": 4,
          "message": 5
        }
      ],
      "owner": "pylint-0"
    },
    {
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^([^:]+):(\\d+):(\\d+): ([A-DF-Z]\\d+): (.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "code": 4,
          "message": 5
        }
      ],
      "owner": "pylint-1"
    }
  ]
}
